        <header class="base-header">
            <div class="container base-header__container">
                <a class="base-header__brand" href="/">
                    <img class="brand-logo" src="/assets/img/logo.png" alt="Logo Thumbnail">
                    <div>
                        <p class="brand-title">{{ siteName }}</p>
                        <small class="brand-subtitle">{{ siteSubtitle }}</small>
                    </div>
                </a>
                <div class="base-header__default-nav">
                    {% include '_base-nav.njk' %}
                </div>
            </div>
        </header>
        <main class="base-content">{{ content | safe }}</main>
        <footer class="base-footer">
            <section class="container base-footer__container">
                {% include '_base-nav.njk' %}
                <small class="footnote">
                    &copy; {% currentYear %} - Developed by <a href="{{ githubProfile }}" target="_blank">{{ author }}</a>
                </small>
            </section>
        </footer>
        <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4="
            crossorigin="anonymous"></script>
        <script src="/assets/js/script.min.js" async></script>
    </body>
</html>